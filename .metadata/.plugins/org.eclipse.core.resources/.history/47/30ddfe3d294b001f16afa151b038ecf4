package methodTest;

public class MethodTask2 {
	
//  (a)
//  이름을 출력해주는 메소드
//	void printName(String name) {
//		System.out.println(name);
//	}
//  1부터 10까지 짝수만 출력해주는 메소드
	void printEven() {
		for(int i = 0; i < 10; i++) {
			System.out.println(i % 2 == 0 ? i : "없음");
		}
	}
	public static void main(String[] args) {
		MethodTask2 mt = new MethodTask2();
		mt.printEven();
	}
//  10을 5번 출력해주는 메소드
//  나이에 10을 더해주는 메소드
//  두 정수를 곱해주는 메소드
//  세 정수를 더해주는 메소드 
	
}
