//		1~100 까지 담고 짝수의 합을 출력
		int[] arData1 = new int[100];
		int sum = 0;
		
		for (int i = 0; i < arData1.length; i++) {
			arData1[i] = i + 1;
			if ( i % 2 != 0) {
			sum += arData1[i];
		    }
		}
			System.out.println(sum);
	
//		A~F까지 담고 출력
		int[] numLetter = new int[6];
		char realLetter = ' ';
		
		
		for (int i = 0; i < numLetter.length; i++) {
			numLetter[i] = i + 1;
			realLetter = (char)('A' + i);
		}
			System.out.println(realLetter);
	
		
//		A~F까지중 C를 제외하고 배열에 담고 출력
