//			if (data.charAt(i) == 65) {
//				System.out.println("응 A있어!");
		}
		
//	    E 출력하기
//			System.out.println(data.charAt(data.indexOf('E')));
			
		
//		D 출력하기
//			System.out.println(data.charAt(data.indexOf('D')));
		for(int i = 0; i < data.length(); i++) {
			data.charAt(i) = 0;
		}
        
		
		
		}
	}

