package arrayTest;

public class ArrTest2 {
	public static void main(String[] args) {
		String data = "ABCDEFG";
		System.out.println(data.length()); //length라는 메서드를 통해 길이 구할수 있음
		for(int i = 0; i < data.length(); i++) {
			System.out.println(data.charAt(i)); //추출할 수 있음(char)
			if (data.charAt(i) == 65) {
				System.out.println("응 A있어!");
			}
		}
	}
}
