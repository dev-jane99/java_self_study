package study0728;

public class ArrayStudy2 {
	public static void main(String[] args) {
		
//		Q. A~F까지 담고 출력
		
//		배열 선언
        char[] english = new char[5];
        
//      초기값
        int numa = 0;
        int numf = 0;
      
//      조건 입력하고 출력
        for(int i = 0; i < english.length; i++) {
        	numa = i + 65;
        	numf = i + 69;
            english[i] = (char)(numa >= numf);
            System.out.println((english[i]));
        }
        
//       65 <= i <= 69
	}
}
